                    <journal-title>PsyArXiv</journal-title>
                    <abbrev-journal-title abbrev-type="publisher">PsyArXiv</abbrev-journal-title>
                </journal-title-group>
                <publisher>
                    <publisher-name>Center for Open Science</publisher-name>
                </publisher>
            </journal-meta>
            <article-meta>
                <article-id pub-id-type="doi">10.31234/osf.io/tb26v</article-id>
            </article-meta>
        </front>
    </article>
