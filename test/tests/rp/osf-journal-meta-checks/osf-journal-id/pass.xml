<?oxygen SCHSchema="osf-journal-id.sch"?>
<!--Context: article/front/journal-meta[lower-case(journal-id[1])='osf preprints']
Test: assert    journal-id[@journal-id-type='publisher-id']='OSF Preprints'
Message: Preprints on OSF must have a <journal-id journal-id-type="publisher-id"> element with the value 'OSF Preprints'. -->
<root xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink">
    <article>
        <front>
            <journal-meta>
                <journal-id journal-id-type="publisher-id">OSF Preprints</journal-id>
                <journal-title-group>
                    <journal-title>OSF Preprints</journal-title>
                    <abbrev-journal-title abbrev-type="publisher">OSF pre.</abbrev-journal-title>
                </journal-title-group>
                <publisher>
                    <publisher-name>Center for Open Science</publisher-name>
                </publisher>
            </journal-meta>
            <article-meta>
                <article-id pub-id-type="doi">10.31219/osf.io/tb26v</article-id>
            </article-meta>
        </front>
    </article>
</root>