<?oxygen SCHSchema="osf-abbrev-journal-title.sch"?>
<!--Context: article/front/journal-meta[lower-case(journal-id)='osf preprints']
Test: assert    journal-title-group/abbrev-journal-title[@abbrev-type='publisher']='OSF pre.'
Message: Preprints on OSF must have a <abbrev-journal-title abbrev-type="publisher"> element with the value 'OSF pre.' inside a <journal-title-group> element. -->
<root xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink">
    <article>
        <front>
            <journal-meta>
                <journal-id journal-id-type="publisher-id">OSF preprints</journal-id>
                <journal-title-group>
                    <journal-title>OSF preprints</journal-title>
                    <abbrev-journal-title abbrev-type="publisher">OSF preprints</abbrev-journal-title>
                </journal-title-group>
                <publisher>
                    <publisher-name>Center for Open Science</publisher-name>
                </publisher>
            </journal-meta>
            <article-meta>
                <article-id pub-id-type="doi">10.31219/osf.io/tb26v</article-id>
            </article-meta>
        </front>
    </article>
</root>