<?oxygen SCHSchema="arxiv-publisher.sch"?>
<!--Context: article/front/journal-meta[lower-case(journal-id[1])='arxiv']
Test: assert    publisher/publisher-name='Cornell University'
Message: arXiv preprints must have a <publisher-name> element with the value 'Cornell University', inside a <publisher> element. -->
<root xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink">
    <article>
        <front>
            <journal-meta>
                <journal-id journal-id-type="publisher-id">arXiv</journal-id>
                <journal-title-group>
                    <journal-title>arXiv</journal-title>
                    <abbrev-journal-title abbrev-type="publisher">arXiv</abbrev-journal-title>
                </journal-title-group>
                <issn pub-type="epub">2331-8422</issn>
                <publisher>
                    <publisher-name>arXiv University</publisher-name>
                </publisher>
            </journal-meta>
            <article-meta>
                <article-id pub-id-type="doi">10.48550/arXiv.1111.1111</article-id>
            </article-meta>
        </front>
    </article>
</root>