<?oxygen SCHSchema="arxiv-journal-id.sch"?>
<!--Context: article/front/journal-meta[lower-case(journal-id)='arxiv']
Test: assert    journal-id[@journal-id-type='publisher-id']='arXiv'
Message: arXiv preprints must have a <journal-id journal-id-type="publisher-id"> element with the value 'arXiv'. -->
<root xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink">
    <article>
        <front>
            <journal-meta>
                <journal-id journal-id-type="publisher-id">arXiv</journal-id>
                <journal-title-group>
                    <abbrev-journal-title abbrev-type="publisher">arXiv</abbrev-journal-title>
                </journal-title-group>
                <issn pub-type="epub">2331-8422</issn>
            </journal-meta>
            <article-meta>
                <article-id pub-id-type="doi">10.48550/arXiv.1111.1111</article-id>
            </article-meta>
        </front>
    </article>
</root>