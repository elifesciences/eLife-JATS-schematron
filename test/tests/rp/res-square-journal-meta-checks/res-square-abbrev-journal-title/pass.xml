<?oxygen SCHSchema="res-square-abbrev-journal-title.sch"?>
<!--Context: article/front/journal-meta[lower-case(journal-id[1])='rs']
Test: assert    journal-title-group/abbrev-journal-title[@abbrev-type='publisher']='rs'
Message: Research Square preprints must have a <abbrev-journal-title abbrev-type="publisher"> element with the value 'rs' inside a <journal-title-group> element. -->
<root xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink">
    <article>
        <front>
            <journal-meta>
                <journal-id journal-id-type="publisher-id">rs</journal-id>
                <journal-title-group>
                    <journal-title>Research Square</journal-title>
                    <abbrev-journal-title abbrev-type="publisher">rs</abbrev-journal-title>
                </journal-title-group>
                <publisher>
                    <publisher-name>Research Square</publisher-name>
                </publisher>
            </journal-meta>
            <article-meta>
                <article-id pub-id-type="doi">10.21203/rs.3.rs-11/v1</article-id>
            </article-meta>
        </front>
    </article>
</root>